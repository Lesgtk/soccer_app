<% if @feed_items.any? %>
  <ol class="posts">
    <% if @search_posts %>
      <%= render @search_posts %>
      <%= paginate @search_posts, theme: 'twitter-bootstrap-4' %>
    <% else %>
      <%= render @feed_items %>
      <%= paginate @feed_items, theme: 'twitter-bootstrap-4' %>
    <% end %>
  </ol>
<% end %>
